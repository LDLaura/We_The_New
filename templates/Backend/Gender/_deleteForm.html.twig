<form action={{ path('admin.gender.delete', {id: gender.id}) }} method="POST" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette categorie ?')">
    <input type="hidden" name="token" value="{{ csrf_token('delete' ~ gender.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>